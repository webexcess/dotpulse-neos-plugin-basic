prototype(Page) {
	head.@process {
		compression >
		cleanHTML = Basic:CleanHTML
	}
	body.@process {
		compression >
		cleanHTML = Basic:CleanHTML
	}
}

prototype(Basic:CleanHTML) < prototype(TS:Value) {
	regex = ${'/(?>[^\S ]\s*| \s{2,})' + baseConfiguration.basicRegex + '/i'}
	value = ${String.pregReplace(value, this.regex, ' ')}
	@position = 'end 999999'
	@if.isLive = ${node.context.live}
}
